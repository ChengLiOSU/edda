<ServerManagerConfiguration>
  <ProxyGroup name="sources">
    <!-- ================================================================== -->
    <SourceProxy name="EddaReader" class="vtkEddaReader" label="EddaReader">
      <Documentation
         long_help="Read an Edda file."
         short_help="Read an Edda file.">
      </Documentation>
      <StringVectorProperty
            name="FileName"
            animateable="0"
            command="SetFileName"
            number_of_elements="1">
        <FileListDomain name="files"/>
        <Documentation>
          This property specifies the file name for the Edda reader.
        </Documentation>
      </StringVectorProperty>
 
      <Hints>
        <ReaderFactory extensions="info"
                       file_description="Edda Format" />
     </Hints>
    </SourceProxy>
    <!-- End Reader -->
  </ProxyGroup>
  <!-- End Filters Group -->


  <ProxyGroup name="filters">
    <!-- ================================================================== -->
    <SourceProxy name="EddaUncertainIsocontour" class="vtkUncertainIsocontour" label="EddaUncertainIsocontour">
      <Documentation
         long_help="Compute uncertain isocontour with level crossing probability."
         short_help="Uncertain Isocontour">
      </Documentation>

     <InputProperty
        name="Input"
        command="SetInputConnection">
           <ProxyGroupDomain name="groups">
             <Group name="sources"/>
             <Group name="filters"/>
           </ProxyGroupDomain>
           <DataTypeDomain name="input_type">
             <DataType value="vtkDataSet"/>
           </DataTypeDomain>
      </InputProperty>

  <DoubleVectorProperty name="Isovalue"
                        command="SetIsov"
                        number_of_elements="1"
                        default_values="1">
  </DoubleVectorProperty>

      <Hints>
    <ShowInMenu category="EDDA" />
      </Hints>
 
    </SourceProxy>

    <!-- ================================================================== -->
    <SourceProxy name="EddaRandomSampleField" class="vtkRandomSampleField" label="EddaRandomSampleField">
      <Documentation
         long_help="Obtain random samples from distribution data."
         short_help="Random sampling">
      </Documentation>

     <InputProperty
        name="Input"
        command="SetInputConnection">
           <ProxyGroupDomain name="groups">
             <Group name="sources"/>
             <Group name="filters"/>
           </ProxyGroupDomain>
           <DataTypeDomain name="input_type">
             <DataType value="vtkDataSet"/>
           </DataTypeDomain>
      </InputProperty>

      <Hints>
    <ShowInMenu category="EDDA" />
      </Hints>
 
    </SourceProxy>
    <!-- End filter -->
  </ProxyGroup>
  <!-- End Filters Group -->
</ServerManagerConfiguration>
